<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane id="AnchorPane" fx:id="main" xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.hybrid.controllers.TypingMainController">
   <children>
      <VBox id="vBox" alignment="CENTER" prefHeight="300.0" prefWidth="300.0" spacing="40.0" AnchorPane.bottomAnchor="70.0" AnchorPane.leftAnchor="150.0" AnchorPane.rightAnchor="150.0" AnchorPane.topAnchor="70.0">
         <children>
            <Label id="lbl1" fx:id="label" text="If you want start, choose a level." />
            <Button id="button" fx:id="button" mnemonicParsing="false" prefWidth="100.0" text="%button.text" />
            <Button id="button1" fx:id="button1" mnemonicParsing="false" prefWidth="100.0" text="%button1.text" />
            <Button id="button2" fx:id="button2" mnemonicParsing="false" prefWidth="100.0" text="%button2.text" />
            <Label id="lblManual" fx:id="lblManual" text="%lblManual.text" />
            <Label id="lblRank" fx:id="lblRank" text="Score" />
         </children>
      </VBox>
   </children>
</AnchorPane>
