<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.chart.PieChart?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>

<BorderPane id="bdpane" prefHeight="514.0" prefWidth="635.0" stylesheets="@square_chart.css" xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1">
   <top>
      <Pane fx:id="topPane" prefHeight="100.0" prefWidth="635.0" BorderPane.alignment="CENTER" />
   </top>
   <bottom>
      <Pane fx:id="bottomPane" prefHeight="52.0" prefWidth="600.0" BorderPane.alignment="CENTER" />
   </bottom>
   <center>
      <VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0" BorderPane.alignment="CENTER">
         <children>
            <PieChart id="pieChart" fx:id="pieChart" prefHeight="290.0" prefWidth="635.0" stylesheets="@square_chart.css">
               <cursor>
                  <Cursor fx:constant="NONE" />
               </cursor>
            </PieChart>
            <Button fx:id="btnBack" mnemonicParsing="false" text="Back" textAlignment="CENTER" />
         </children>
      </VBox>
   </center>
</BorderPane>
