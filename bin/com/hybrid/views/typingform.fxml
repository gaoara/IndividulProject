<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<BorderPane xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1">
   <bottom>
      <HBox alignment="CENTER" prefHeight="67.0" prefWidth="200.0" BorderPane.alignment="CENTER">
         <children>
            <Button id="btnAdd2" fx:id="btnAdd2" alignment="CENTER" contentDisplay="CENTER" maxWidth="1.7976931348623157E308" mnemonicParsing="false" text="저장">
               <HBox.margin>
                  <Insets />
               </HBox.margin>
            </Button>
            <Button id="btnCancel" fx:id="btnCancel" maxWidth="1.7976931348623157E308" mnemonicParsing="false" text="취소">
               <HBox.margin>
                  <Insets left="10.0" />
               </HBox.margin>
            </Button>
         </children>
      </HBox>
   </bottom>
   <top>
      <Label fx:id="lblName" text="Typing your name" textFill="#5ea9d7" BorderPane.alignment="CENTER">
         <font>
            <Font name="Agency FB Bold" size="30.0" />
         </font>
      </Label>
   </top>
   <center>
      <TextField fx:id="txtName" BorderPane.alignment="CENTER" />
   </center>
</BorderPane>
